{
  "expo": {
    "name": "PlantCare",
    "slug": "PlantCare",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/favicon.webp",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/favicon.webp",
      "resizeMode": "contain",
      "backgroundColor": "#26D041"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      },
      "bundleIdentifier": "com.plant.Plantcare",
      "permissions": [
        "CAMERA",
        "CAMERA_ROLL",
        "LOCATION",
        "NOTIFICATIONS",
        "PHOTO_LIBRARY",
        "MICROPHONE",
        "CALENDAR",
        "CONTACTS",
        "MOTION",
        "BLUETOOTH"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/favicon.webp",
        "backgroundColor": "#ffffff"
      },
      "package": "com.plant.Plantcare",
      "permissions": [
        "INTERNET",
        "CAMERA",
        "RECORD_AUDIO",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "VIBRATE",
        "WAKE_LOCK",
        "FOREGROUND_SERVICE",
        "RECEIVE_BOOT_COMPLETED",
        "USE_FINGERPRINT"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.webp"
    },
    "extra": {
      "API_BASE_URL": "https://ag-world-orginal.vercel.app/",
      "SHOUTOUT_API_KEY": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..ayaQjSjBxcSSnqskZp_F_NlrLa_98ddiOi1lfK8WrJ4",
      "eas": {
        "projectId": "e2d5fb7c-4ebd-4a32-b488-0d68840c4216"
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/favicon.webpr",
          "color": "#ffffff"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "expo-camera": {
      "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
      "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
      "recordAudioAndroid": true
    },
    "newArchEnabled": true
  }
}
