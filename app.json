{
  "expo": {
    "name": "PlantCare",
    "slug": "PlantCare",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/favicon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/favicon.png",
      "resizeMode": "contain",
      "backgroundColor": "#26D041"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      },
      "bundleIdentifier": "com.plant.Plantcare"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/favicon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.plant.Plantcare",
      "permissions": [
        "INTERNET"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "API_BASE_URL": "https://ag-world-orginal.vercel.app/",
      "SHOUTOUT_API_KEY": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..ayaQjSjBxcSSnqskZp_F_NlrLa_98ddiOi1lfK8WrJ4",
      "eas": {
        "projectId": ""
      }
    },
    "plugins": [
      "expo-router",
      "expo-notifications",
      {
        "icon": "./local/assets/notification_icon.png",
        "color": "#ffffff",
        "defaultChannel": "default",
        "sounds": [
          "./local/assets/notification_sound.wav",
          "./local/assets/notification_sound_other.wav"
        ],
        "enableBackgroundRemoteNotifications": false
      }
    ],
    "experiments": {
      "typedRoutes": true
    },
    "expo-camera": {
      "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
      "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
      "recordAudioAndroid": true
    }
  }
}
